local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("GAMEDES Hum", "LightTheme")
local Tab = Window:NewTab("Aoto Farm")
local Section = Tab:NewSection(" ตีออโต้")
Section:NewToggle("ออโต้ตี", "ToggleInfo", function(state)
   _G.aoto = state
while _G.aoto do wait(0)
local args = {
    [1] = Vector3.new(-132.3590087890625, 406.3043212890625, 3879.48583984375),
    [2] = workspace:WaitForChild("NewOrganized"):WaitForChild("Useless7"):WaitForChild("New Destroyed City"):WaitForChild("City"):WaitForChild("Parking Garage"):WaitForChild("Part")
}
game:GetService("Players").LocalPlayer.Character.Main.Swing:FireServer(unpack(args))
end
end)
local Section = Tab:NewSection(" aoto farm")
Section:NewToggle("ตี mon", "", function(state)
_G.Aoto = state
         while _G.Aoto do wait()
             pcall(function()
               for i,v in pairs(workspace.NPCs:GetDescendants()) do
            if v.Name == "High End 2" then
            if v.Humanoid.Health >= 0 then
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(3,7,4)
               end
             end
          end
       end)
   end
end)
Section:NewButton("aoto Quest", "", function()
    local args = {
    [1] = "QUEST_MIRKO_1"
}

game:GetService("ReplicatedStorage"):WaitForChild("Questing"):WaitForChild("Networking"):WaitForChild("Remotes"):WaitForChild("QUESTING_START_QUEST"):FireServer(unpack(args))
end)
local Section = Tab:NewSection(" Boss")
Section:NewToggle("boss โทมุระ", "", function(state)
    _G.Aoto = state
         while _G.Aoto do wait()
             pcall(function()
               for i,v in pairs(workspace.NPCs:GetDescendants()) do
            if v.Name == "Tomura" then
            if v.Humanoid.Health >= 0 then
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(3,7,4)
               end
             end
          end
       end)
   end
end)
Section:NewToggle("boss ดาบิ", "", function(state)
    _G.Aoto = state
         while _G.Aoto do wait()
             pcall(function()
               for i,v in pairs(workspace.NPCs:GetDescendants()) do
            if v.Name == "Dabi" then
            if v.Humanoid.Health >= 0 then
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(3,7,4)
               end
             end
          end
       end)
   end
end)
Section:NewToggle("bossโนมุ", "", function(state)
   G.Aoto = state
         while _G.Aoto do wait()
             pcall(function()
               for i,v in pairs(workspace.NPCs:GetDescendants()) do
