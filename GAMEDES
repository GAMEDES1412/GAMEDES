               end
             end
          end
       end)
   end
end)
Section:NewToggle("bossออหอวัน", "", function(state)
_G.Aoto = state
         while _G.Aoto do wait()
             pcall(function()
               for i,v in pairs(workspace.NPCs:GetDescendants()) do
            if v.Name == "AllForOne" then
            if v.Humanoid.Health >= 0 then
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(3,7,4)
               end
             end
          end
       end)
   end
end)
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection(" WalkSpeed")
Section:NewSlider("วิ่ง", "", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
local Tab = Window:NewTab("UI")
local Section = Tab:NewSection(" ซ่อนUI")
Section:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
